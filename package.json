  "description": "Easily create a service and register it with Consul in node.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc --reporter=lcov mocha --compilers js:babel-register test/specs",
    "build": "babel src -d dist",
    "dev": "babel src -d dist -w",
    "cov": "open ./coverage/lcov-report/index.html",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samstefan/consul-service-wrapper.git"
  },
  "author": "Sam Stefan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samstefan/consul-service-wrapper/issues"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  },
  "homepage": "https://github.com/samstefan/consul-service-wrapper#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "should": "^11.2.1",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "consul": "^0.28.0"
  }
